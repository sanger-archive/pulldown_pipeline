<div
         id="<%= "aliquot_#{id}" %>"
      class="aliquot <%= tag_colour(container) %> <%= container.location %> <%= column(container) %> col-all"
        rel="<%= "#details_#{id}" %>"
  data-pool="<%= container.respond_to?(:pool) ? container.pool_id : '' %>"
  ><%= container.aliquots.first.tag.try(:identifier) || '&nbsp;' %></div>

